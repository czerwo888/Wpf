<Window x:Class="powtorzenie.addWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:powtorzenie"
        mc:Ignorable="d"
        Title="addWindow" Height="450" Width="400">
    <StackPanel>
        <TextBox x:Name="nameBox"></TextBox>
        <TextBox x:Name="ageBox"></TextBox>
        <Button Content="Dodaj" Click="add"></Button>
    </StackPanel>
</Window>
